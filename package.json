{
  "name": "pyramid-launcher",
  "main": "dist/index.js",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"vite\" \"wait-on tcp:5648 && yarn electron\"",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "electron": "nodemon --watch electron/ --exec \"swc ./electron/index.ts -d ./dist && npx electron .\" electron/index.ts",
    "fixbsqlite": "cd node_modules/better-sqlite3 && node-gyp rebuild --debug --build-from-source --runtime=electron --target=26.1.0 --dist-url=https://electronjs.org/headers && cd ../.."
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@pyramidmc/tree": "../pmc-tree/",
    "@xmcl/core": "^2.12.0",
    "@xmcl/installer": "^5.1.0",
    "@xmcl/user": "^3.0.3",
    "chakra-react-select": "^4.7.0",
    "framer-motion": "^10.16.1",
    "isomorphic-ws": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "ws": "^8.14.1"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.3.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.78",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "concurrently": "^8.2.1",
    "electron": "^26.1.0",
    "electron-builder": "^24.6.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.5",
    "wait-on": "^7.0.1"
  }
}
